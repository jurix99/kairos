# Configuration pour Docker Compose
# Copiez ce fichier vers .env et remplissez les valeurs

# Base de données PostgreSQL
POSTGRES_DB=kairos
POSTGRES_USER=kairos_user
POSTGRES_PASSWORD=kairos_password

# Backend
DATABASE_URL=postgresql://kairos_user:kairos_password@postgres:5432/kairos

# OAuth GitHub (requis pour l'authentification)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# CORS (URLs autorisées)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# API de calcul de temps de trajet (optionnel)
# Décommentez et configurez pour utiliser une API de cartographie
# TRAVEL_API_PROVIDER=google  # Options: google, mapbox, openroute
# TRAVEL_API_KEY=your_api_key_here
# USE_TRAVEL_API=true

# Frontend
VITE_API_URL=http://localhost:8080 